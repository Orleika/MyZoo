<% galleryHelper = (gallery) -> %>
  <div class="list">
    <img class="list_view" <%- gallery.image %>" alt="<%- gallery.name %>">
    <h3><%- gallery.name %></h3>
    <p><%- gallery.uname %></p>
  </div>
<% end %>

<% if @galleries?.length : %>
  <ul>
    <% for gallery in @galleries : %>
      <%- galleryHelper gallery %>
    <% end %>
  </ul>
<% else : %>
  <p>List is empty</p>
<% end %>